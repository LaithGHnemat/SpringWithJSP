</head>
<body>
 <p id="date"></p>
 <div class="container">
  <h2>Employee Dto  List</h2>
  <table class="table">
   <thead>
    <th scope="row">Name</th>
    <th scope="row">salary</th>
    <th scope="row">Location</th>

   </thead>
   <tbody>
    <c:forEach items="${ListEmployeebyDTO}" var="LED" >
     <tr>
      <td>${LED.name}</td>
      <td>${LED.salary}</td>
      <td>${LED.location}</td>




     </tr>
    </c:forEach>
   </tbody>
  </table>

 </div>
  <script src="<c:url value="/js/timee.js"></c:url>"></script>

	
</body>
</html>